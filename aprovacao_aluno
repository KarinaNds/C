#include <stdio.h>

int main() {

  int nota, frequencia;

  printf("Digite a nota do aluno: ");
  scanf("%d", &nota);

  printf("Digite a frequencia do aluno: ");
  scanf("%d", &frequencia);

  if(nota >= 6 && frequencia >= 75){
    printf("O aluno foi aprovado");
  }
  else{
    printf("O aluno foi reprovado");
  }

  return 0;
}
